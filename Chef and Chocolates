import java.util.*;
class main{
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        int t = input.nextInt();
        int a,b,c;
        for(int i=0;i<t;i++){
            a=input.nextInt();
            b=input.nextInt();
            c=input.nextInt();
            int d = a*5+b*10;
            System.out.println((int)d/c);
        }
    }
}
