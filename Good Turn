import java.util.*;
class main{
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        int t = input.nextInt();
        for(int i=0;i<t;i++){
            int n = input.nextInt();
            int m = input.nextInt();
            if(n+m>6)
            System.out.println("yes");
            else
            System.out.println("no");
        }
    }
}
